package com.gmail.shinigamihideo;

public class Cat {
	private String name;
	private double weight;
	private String color;
	private int age;
	private String breed;
	
	public Cat(String name, double weight, String color, int age, String breed) {
		super();
		this.name = name;
		this.weight = weight;
		this.color = color;
		this.age = age;
		this.breed = breed;
	}
	@Override
	public String toString() {
		return "Cat [name=" + name + ", weight=" + weight + ", color=" + color + ", age=" + age + ", breed=" + breed
				+ "]";
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getWeight() {
		return weight;
	}
	public void setWeight(double weight) {
		this.weight = weight;
	}
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getBreed() {
		return breed;
	}
	public void setBreed(String breed) {
		this.breed = breed;
	}

	

	
}
