package com.gmail.shinigamihideo;

public class Main {

	public static void main(String[] args) {
		Cat cat1 = new Cat("Bob", 8, "Black", 5, "Siam");
		Cat cat2 = new Cat("Chak", 3, "White", 3, "yardcat");
		System.out.println(cat1);
		System.out.println(cat2);
		
		System.out.println();

		Triangle tr1 = new Triangle(4, 5, 3);
		tr1.areaTr();
		
		Triangle tr2 = new Triangle(7, 6, 9);
		tr2.areaTr();
		
		Triangle tr3 = new Triangle(5, 8, 5);
		tr3.areaTr();

	}
}
